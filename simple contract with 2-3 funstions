pragma solidity ^0.8.13;

/**
   * @title ContractName
   * @dev ContractDescription
   * @custom:dev-run-script file_path
   */
  contract ContractName {}

contract hello {
    string public name;
    string public greetings = "Konichiwa";

    function setVars(string memory initialName) public payable {
        name = initialName;   
    }
    function setVarsi(string memory newName) public payable {
        name = newName;
        
    }
}
